<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.techease.btcConverter">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:allowBackup="true"
        android:icon="@drawable/bitcoin"
        android:label="@string/app_name"
        android:roundIcon="@drawable/bitcoin"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <service
            android:name="com.techease.btcConverter.NotificationService"
            android:label="@string/app_name">

        </service>

        <activity android:name="com.techease.btcConverter.LoginActivity">

        </activity>
        <activity android:name="com.techease.btcConverter.SignupActivity" />
        <activity android:name="com.techease.btcConverter.MainActivity_conversion">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


    </application>

</manifest>